@{
ViewData["Title"] = "Semester Plan";
}
<div class="content-wrapper" style="min-height: 912.2px;">
	<!-- Content Header (Page header) -->
	<header class="content-header">
		<div class="container-fluid">
			<div class="row mb-2">
				<div class="col-sm-6">
					<h1 class="m-0">Kế Hoạch</h1>
				</div><!-- /.col -->
			</div><!-- /.row -->
		</div><!-- /.container-fluid -->
	</header>
	<!-- /.content-header -->

	<!-- Main content -->
	<main class="content">
		<div class="container-fluid">
			<div class="card">
				<div class="card-header">
					<div class="row text-center">

						<div class="col-auto">
							<button type="button" class="btn btn-success" onclick="AddPlans()">
								<i class="bi bi-plus"></i> Thêm Kế Hoạch
							</button>
							<button type="button" class="btn btn-secondary" onclick="exportPlans()">
								<i class="bi bi-download custom-icon"></i> Export
							</button>
						</div>

						<div class="col">
							<nav class="navbar navbar-light bg-light">
								<form class="d-flex ms-auto">
									<input class="form-control me-2" type="search" placeholder="Nhập nội dung tìm kiếm" aria-label="Search">
								</form>
							</nav>
						</div>
					</div>
					<!-- Filter Combo Box -->
					<div class="row mt-2">
						<div class="col-4 text-left">
							<label for="statusFilter" class="form-label">Trạng Thái</label>
							<select id="statusFilter" class="form-select custom-select" style="width: 150px;" onchange="filterTable()">
								<option value="">Tất cả</option>
								<option value="Đã nộp">Đã Nộp</option>
								<option value="Chưa nộp">Chưa Nộp</option>
								<option value="Nháp">Nháp</option>
							</select>
						</div>
					</div>

				</div>
				<!-- /.card-header -->

				<div class="card-body">
					<table class="table table-bordered table-striped">
						<thead>
						<tr>
							<th>#</th> <!-- Cột ID đã thêm -->
							<th>Năm Học</th>
							<th>Loại Kế Hoạch</th>
							<th>Mã Lớp</th>
							<th>Tên Cố Vấn</th>
							<th>Ngày Tạo</th>
							<th>Minh Chứng</th>
							<th>Trạng Thái</th>
							<th>Thao Tác</th>
						</tr>
						</thead>
						<tbody id="data-table">
						<tr>
							<td>1</td> 
							<td>2024 - 2025</td>
							<td>Kế Hoạch CVHT 2024-2025</td>
							<td>71K27CNTT01</td>
							<td>Lý Thị Huyền Châu</td>
							<td>08/08/2024</td>
							<td><input type="file" name="ImageUpload" accept="image/*"/>
							</td>
							<td>Nháp</td>
							<td>
								<a href="/Categories/SemesterPlanDetail" class="btn btn-outline-success btn-sm" title="Xem chi tiết">
									<i class="bi bi-eye"></i>
								</a>

								<button class="btn btn-outline-primary btn-sm" title="Cập Nhật">
									<i class="bi bi-pencil"></i>
								</button>

								<button class="btn btn-outline-danger btn-sm delete" title="Xóa">
									<i class="bi bi-trash"></i>
								</button>
								<button class="btn btn-outline-secondary btn-sm" title="Nộp ">
									<i class="bi bi-file-earmark-arrow-up"></i>
								</button>
							</td>
						</tr>
						<tr>
							<td>2</td>
							<td>2024 - 2025</td>
							<td>Kế Hoạch CVHT 2024-2025</td>
							<td>71K27CNTT02</td>
							<td>Nguyễn Văn A</td>
							<td>08/08/2024</td>
							<td><img src="data:image/jpeg;base64," alt="Ảnh hiển thị" style="max-width:80px; max-height: 80px; cursor: pointer;" onclick="showImage(this.src)"><i class="bi bi-x" style="float:right;"></i>
							</td>
							<td>Chưa nộp</td>
							<td>
								<a href="/Categories/SemesterPlanDetail" class="btn btn-outline-success btn-sm" title="Xem chi tiết">
									<i class="bi bi-eye"></i>
								</a>
								<button class="btn btn-outline-primary btn-sm" title="Cập Nhật">
									<i class="bi bi-pencil"></i>
								</button>

								<button class="btn btn-outline-danger btn-sm delete" title="Xóa">
									<i class="bi bi-trash"></i>
								</button>
								<button class="btn btn-outline-secondary btn-sm" title="Nộp ">
									<i class="bi bi-file-earmark-arrow-up"></i>
								</button>
							</td>
						</tr>
						<!-- Thêm nhiều dữ liệu mẫu ở đây -->
						</tbody>
					</table>

					<!-- Pagination -->
					<nav aria-label="Page navigation">
						<ul class="pagination justify-content-end">
							<li class="page-item disabled">
								<a class="page-link" aria-label="Previous">
									<span aria-hidden="true">«</span>
								</a>
							</li>

							<li class="page-item active"><a class="page-link">1</a></li>

							<li class="page-item">
								<a class="page-link" aria-label="Next" href="#">
									<span aria-hidden="true">»</span>
								</a>
							</li>
						</ul>
					</nav>
				</div>
				<!-- /.card-body -->
			</div>
		</div>
	</main>
	<!-- Modal để hiển thị ảnh lớn -->
	<div id="imageModal" style="display: none; position: fixed; top: 0; left: 0; width: 100%; height: 100%; background-color: rgba(0, 0, 0, 0.8); justify-content: center; align-items: center;">
		<span onclick="closeModal()" style="color: white; font-size: 30px; position: absolute; top: 20px; right: 30px; cursor: pointer;">&times;</span>
		<img id="modalImage" style="max-width: 90%; max-height: 90%;">
	</div>
</div>